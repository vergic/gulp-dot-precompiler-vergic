  "dependencies": {
    "doT-vergic": "https://github.com/vergic/doT-vergic/archive/1.1.3-vergic.tar.gz",
    "gulp-util": "~2.2.20",
    "lodash": "~4.17.21",
    "through2": "~2.0.5"
  },
  "devDependencies": {
    "coveralls": "~3.1.0",
